          <xs:documentation>Корр. счет банка получателя</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="BillingInfoGUID" type="tns:GUIDType">
    <xs:annotation>
      <xs:documentation>Идентификатор платежных реквизитов ЮЛ/ИП</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="OKIGuid" type="tns:GUIDType">
    <xs:annotation>
      <xs:documentation>Идентификатор ОКИ в ГИС ЖКХ</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="Ind" type="tns:IndType" />
</xs:schema>