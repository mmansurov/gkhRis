        <xs:minLength value="1" />
        <xs:maxLength value="30" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="AccountUniqueNumber" type="xs:string">
    <xs:annotation>
      <xs:documentation>Уникальный номер ЛС, присвоенный ГИС ЖКХ</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="PaymentReasonType">
