    <xs:annotation>
      <xs:documentation>Счет</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:length value="20" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="PaymentInformationType">
